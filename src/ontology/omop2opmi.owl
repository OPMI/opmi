    <!-- http://purl.obolibrary.org/obo/OPMI_0000543 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OPMI_0000543">
        <rdfs:label xml:lang="en">cause of death status</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OPMI_0000533"/>
        <obo:IAO_0000119>https://athena.ohdsi.org/search-terms/terms?domain=Condition+Status</obo:IAO_0000119>
        <obo:IAO_0000115>A cause of death is an official determination of conditions resulting in a human&#39;s death, which may be recorded on a death certificate. A cause of death is determined by a medical examiner.</obo:IAO_0000115>
        <obo:IAO_0000119>https://www.cdc.gov/nchs/data/misc/hb_cod.pdf</obo:IAO_0000119>
        <rdfs:seeAlso>OMOP4976961</rdfs:seeAlso>
        <obo:IAO_0000119>https://en.wikipedia.org/wiki/Cause_of_death</obo:IAO_0000119>
        <obo:IAO_0000117>Oliver He, Long Tran</obo:IAO_0000117>
        <obo:IAO_0000118>cause of death</obo:IAO_0000118>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/opmi.owl"/>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OPMI_0000546 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OPMI_0000546">
        <rdfs:label xml:lang="en">secondary diagnosis status</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OPMI_0000533"/>
        <obo:IAO_0000117>Oliver He, Long Tran</obo:IAO_0000117>
        <obo:IAO_0000115>A secondary diagnosis is a condition that coexists at the time of admission, that develops subsequently, or that affects the treatment received and/or length of stay.</obo:IAO_0000115>
